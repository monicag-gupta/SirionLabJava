package day3;
class Age{
	private int a;

	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}
	public Age(int a) {this.a=a;}
	public Age() {}

	@Override
	public String toString() {
		Integer i=a;
		return  i.toString();
	}
	
	
}
final class Emp{
	private String name;
	private final Age age;
	public String getName() {
		return name;
	}
	public Age getAge() {
		return new Age(age.getA());
	}
	public Emp(String name, Age age) {
		this.name = name;
		this.age = new Age(age.getA());
	}
	@Override
	public String toString() {
		return "Emp [name=" + name + ", age=" + age + "]";
	}
	
	
}

public class MyImmutableEmpEg {

	public static void main(String[] args) {
		Age a=new Age(100);
		Emp e=new Emp("John",a);
		System.out.println(e);
		a.setA(120);
		System.out.println(e);
		e.getAge().setA(150);
		System.out.println(e);
	}

}
